#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
#include <iomanip>
using namespace std;


int main() {
    int n, i, j, z = 0;
    cin >> n;
    
    for (i = 1; i <= n; i++)
    {
        for ( j = i; j < n; j++)
        {
            cout << " ";
        }
         for ( z = n-i; z < n; z++)
           {
                cout << "#";
           }
            cout << "\n";
    }
    return 0;
}
